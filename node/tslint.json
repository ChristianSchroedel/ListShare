{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "grouped-imports": false,
    "interface-name": false,
    "arrow-parens": false,
    "no-console": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "member-ordering": [
      "fields-first"
    ]
  }
}
